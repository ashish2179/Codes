@#include"header.h"
#include"prototypes.h"
int main_menu()
{
	char ch;
	int n;
	printf("\t____MAIN MENU____\n");
	printf("1.Compression\n");
	printf("2.Decompression\n");
	printf("3.EXIT\n");
	printf("Enter your choice:-\n");
	scanf("%d",&n);
	while(1)
	{
		switch(n)
		{
			case 1:
				compression();
				break;
			case 2:
				decompression();
				break;
			case 3: exit(EXIT_SUCCESS);
				break;
			default:
				printf("enter valid choice\n");
				break;
		}
		printf("1.Compression\n");
		printf("2.Decompression\n");
		printf("3.EXIT\n");
		printf("Enter your choice:-\n");
		scanf("%d",&n);
	}
		return 0;
	}

